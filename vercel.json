{
  "headers": [
    {
      "source": "/index.html",
      "headers": [
        { "key": "Cache-Control",  "value": "no-store, no-cache, must-revalidate, proxy-revalidate"}
      ]
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "builds": [
    {
      "src": "angular.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ]
}
